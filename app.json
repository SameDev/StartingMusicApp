{
  "name": "Starting Music",
  "expo": {
    "name": "Starting Music",
    "slug": "StartingMusicApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "autolinking": {
      "searchPaths": ["../../packages"]
    },
    "scheme": "startingmusic",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/logo.png", 
      "resizeMode": "contain",
      "backgroundColor": "#202238"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.startingmusicapp.samedev",  
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },
    "android": {
      "package": "com.startingmusicapp.samedev",
      "useNextNotificationsApi": true, 
      "permissions": [
        "CAMERA",
        "MEDIA_LIBRARY",
        "CAMERA_ROLL",
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",  
        "backgroundColor": "#202238"
      },
      "notification": {
        "icon": "./assets/images/logo.png",
        "color": "#202238",
        "sound": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-font",
      "expo-audio",
      "expo-notifications"  
    ],
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY",
      "CAMERA_ROLL",
      "NOTIFICATIONS"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "33f1af2a-b372-482c-ab62-d2312e00bded"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#191A2E",
      "translucent": false,
      "barStryle": "light-content"
    },
    "backgroundColor": "#191A2E",
    "primaryColor": "#312464"
  }
}
