{
  "expo": {
    "name": "StartingMusicApp",
    "slug": "StartingMusicApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://starting-music-artista.vercel.app/_vercel/image?url=/logo.svg&w=1536&q=100",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "https://starting-music-artista.vercel.app/_vercel/image?url=/logo.svg&w=1536&q=100",
      "resizeMode": "contain",
      "backgroundColor": "#191A2E"
    },
    "ios": {
      "supportsTablet": true
    },
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://starting-music-artista.vercel.app/_vercel/image?url=/logo.svg&w=1536&q=100",
        "backgroundColor": "#191A2E"
      },
      "package": "com.StartingMusicApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://starting-music-artista.vercel.app/_vercel/image?url=/logo.svg&w=1536&q=100"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-font",
      "expo-audio"
    ],
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY",
      "CAMERA_ROLL"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "33f1af2a-b372-482c-ab62-d2312e00bded"
      }
    }
  }
}
